@echo off
call "%~dp0\paths.cmd"
call "%~dp0\set_conda_env.cmd"
call activate python36
set GIT_PATH=%EWS_PROGRAMS_PATH%\Git
set DOCKER_PATH=%EWS_PROGRAMS_PATH%\docker-toolbox-portable
SET VIRTUALBOX_HOSTONLY_NIC_TYPE=virtio
SET VIRTUALBOX_DISK_SIZE=150000
SET VIRTUALBOX_CPU_COUNT=12
SET VIRTUALBOX_MEMORY_SIZE=147456
SET PATH=%DOCKER_PATH%\app;%PATH%
SET MACHINE_HOST_CIDR=192.168.99.1/24
SET MACHINE_NAME=default
SET MACHINE_STORAGE_PATH=%DOCKER_PATH%/data/storage
SET MACHINE_RAM=%VIRTUALBOX_MEMORY_SIZE%
SET MACHINE_DISK=%VIRTUALBOX_DISK_SIZE%
SET DOCKER_CERT_PATH=%DOCKER_PATH%/data/storage/machines/%MACHINE_NAME%
SET DOCKER_TLS_VERIFY=1
SET MACHINE_CPU=14
SET NO_PROXY=192.168.99.100
SET DOCKER_HOST=tcp://%NO_PROXY%:2376